<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="ir.elegam.amlakbashi.Activity.MainActivity">

    <include layout="@layout/app_bar_header"
        android:id="@+id/main_app_bar_heade"
        />


    <include layout="@layout/app_bar_footer"
        android:id="@+id/main_app_bar_footer"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/main_app_bar_heade"
        android:layout_above="@id/main_app_bar_footer"
        android:padding="@dimen/_5sdp"
        >

    </LinearLayout>

</RelativeLayout>
